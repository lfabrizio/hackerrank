// use a counter
// use sort
//variable set to input length  minus 1

function birthdayCakeCandles(ar) {
    
    let counter = 0;
    
    let result = ar.sort(function(a, b){
    return a - b
    });
    
    

    for (let i = 0; i < result.length; i++)
    {
    if(result[i] == result[result.length-1]){
    counter++;
        }
    }
    return counter;


}