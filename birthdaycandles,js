// use a counter
// use sort
//variable set to input length  minus 1
// math max and use spread operator adds elements from ar

function birthdayCakeCandles(ar) {
    
    let counter = 0;
    
    let height = Math.max(...ar);
    
    

    for (let i = 0; i < arr.length; i++)
    {
    if(arr[i] == height){
    counter++;
        }
    }
    return counter;


}